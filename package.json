    "@rollup/plugin-node-resolve": "^7.1.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "concat-files": "^0.1.1",
    "rollup": "^1.25.1",
    "rollup-plugin-babel": "^4.3.3",
